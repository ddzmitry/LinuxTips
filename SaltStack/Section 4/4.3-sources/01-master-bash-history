vi /srv/salt/apache.sls
mkdir /srv/salt/_modules
vi /srv/salt/_modules/myutil.py
salt '*' saltutil.sync_modules
salt '*' myutil.something
vi /srv/salt/_modules/myutil.py
salt '*' saltutil.sync_modules
salt '*' myutil.something
vi /srv/salt/apache.sls
